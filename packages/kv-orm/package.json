{
  "name": "kv-orm",
  "version": "0.3.2",
  "description": "A Node.JS ORM for key-value datastores",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "clean:node": "rm -rf package-lock.json node_modules/",
    "lint": "eslint 'src/**/*.ts' && prettier -c '*.md'",
    "format": "prettier --write \"src/**/*.ts\" '*.md'",
    "test": "jest --config=jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GregBrimble/kv-orm.git"
  },
  "keywords": [
    "kv",
    "key-value",
    "orm",
    "model",
    "nodejs",
    "javascript"
  ],
  "author": {
    "name": "Greg Brimble",
    "email": "developer@gregbrimble.com",
    "url": "https://gregbrimble.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GregBrimble/kv-orm/issues"
  },
  "homepage": "https://github.com/GregBrimble/kv-orm#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^24.0.16",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "istanbul-lib-coverage": "^3.0.0",
    "istanbul-lib-report": "^2.0.8",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "gitHead": "7e1fa77ce85e81542db4ba2c968cea0af36286b6"
}
